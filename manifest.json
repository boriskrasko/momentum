{
    "manifest_version": 2,
    "name": "Momentum",
    "version": "1.0",
    "description": "Momentum Extension by boriskrasko",
    
    "background": {
      "persistent": false,
      "scripts": [ "background-scripts/script.js" ]
    },

    "icons": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "128": "images/icon128.png"
    },

    "browser_action": {
      "default_title": "Momentum"
    },

    "chrome_url_overrides": {
      "newtab": "index.html"
    },

    "permissions": [
      "activeTab",
      "geolocation"
    ]
  }
